{
    "manifest_version": 3,
    "name": "Todo List Extension",
    "version": "1.0",
    "description": "A simple todo list extension",
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "permissions": ["storage"],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
  