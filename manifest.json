{
    "manifest_version": 3,
    "name": "Todo List Extension",
    "version": "1.0",
    "description": "A simple todo list extension",
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html"
    }
}
  